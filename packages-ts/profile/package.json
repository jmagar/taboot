{
  "name": "@taboot/profile",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./types": "./src/types.ts"
  },
  "typesVersions": {
    "*": {
      "types": [
        "./src/types.ts"
      ],
      "*": [
        "./src/index.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc --build",
    "check-types": "tsc --noEmit --skipLibCheck",
    "clean": "rm -rf dist .turbo node_modules",
    "dev": "tsc --build --watch",
    "lint": "eslint . --max-warnings 0",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "prettier": "@taboot/prettier-config",
  "dependencies": {
    "@taboot/auth": "workspace:*",
    "@taboot/logger": "workspace:*",
    "@taboot/utils": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "catalog:dev",
    "@taboot/eslint-config": "workspace:^",
    "@taboot/jest-presets": "workspace:*",
    "@taboot/prettier-config": "workspace:*",
    "@taboot/typescript-config": "workspace:*",
    "typescript": "catalog:dev",
    "vitest": "^4.0.3"
  }
}
